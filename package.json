{
  "name": "landing-page",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "postbuild": "react-snap",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "reactSnap": {
    "inlineCss": true,
    "minifyHtml": true,
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "publicPath": "/",
    "source": "dist",
    "destination": "dist",
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox"
    ],
    "include": [
      "/",
      "/post/*"
    ],
    "userAgent": "ReactSnap"
  },
