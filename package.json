    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/stats.js": "^0.17.4",
    "@types/three": "^0.179.0",
    "@types/webxr": "^0.5.22",
    "@vitejs/plugin-react": "^4.4.1",
