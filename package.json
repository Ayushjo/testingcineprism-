    "axios": "^1.11.0",
    "canvas-confetti": "^1.9.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.513.0",
    "motion": "^12.23.12",
    "react": "^19.1.0",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.5.2",
    "react-lazy-load-image-component": "^1.6.3",
